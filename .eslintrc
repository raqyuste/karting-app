{
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": "error",
        "import/no-extraneous-dependencies": 0,
        "no-debugger": 1
    },
    "settings": {
        "import/resolver": {
        "alias": {
            "map": []
        }
    }
    },
    "plugins": ["prettier"],
    "globals": {
        "process": true
    }
}